{
  "name": "express-react-starter",
  "version": "1.0.0",
  "description": "A react express starter project",
  "main": "public/index.js",
  "repository": "https://github.com/samyouaret/express-react-starter.git",
  "author": "samyouaret <samyouaret13@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "yarn run buildcss & webpack && nodemon public/index.js",
    "buildcss": "postcss resources/css/*.css --base --dir public/css --env=dev --watch",
    "watch": "nodemon public/index.js -w & yarn run buildcss & webpack -w --mode development",
    "build": "webpack --mode production",
    "migrate": "sequelize-cli db:migrate",
    "seed": "cross-env NODE_ENV=test sequelize-cli db:seed:all",
    "migrate:reset": "sequelize-cli db:migrate:undo:all && yarn run migrate",
    "pretest": "cross-env NODE_ENV=test yarn run migrate:reset && yarn run seed",
    "test": "cross-env NODE_ENV=test jest --env node -- tests --runInBand"
  },
  "dependencies": {
    "babel-loader": "^8.1.0",
    "bcrypt": "^5.0.0",
    "connect-flash": "^0.1.1",
    "connect-redis": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "csurf": "^1.11.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-validator": "^6.6.0",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "mysql2": "^2.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "redis": "^3.0.2",
    "sequelize": "^6.3.3",
    "session-file-store": "^1.5.0",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@fullhuman/postcss-purgecss": "^3.1.0",
    "autoprefixer": "^10.1.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.10",
    "eslint": "^7.11.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.1.0",
    "nodemon": "^2.0.6",
    "postcss": "^8.2.1",
    "postcss-cli": "^8.3.1",
    "sequelize-cli": "^6.2.0",
    "sqlite3": "^5.0.0",
    "supertest": "^6.0.1",
    "terser-webpack-plugin": "^5.0.3",
    "webpack": "^5.10.2",
    "webpack-cli": "^4.2.0"
  }
}